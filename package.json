{
  "name": "promthubs",
  "version": "2.0.0",
  "description": "Modern, AI-destekli kart tasarlama uygulaması. Gemini AI ve Imagen ile görsellerinizi analiz edin ve yüksek kaliteli kartlar oluşturun.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "node --watch src/server/index.js",
    "lint": "eslint src/ config/ --ext .js",
    "lint:fix": "eslint src/ config/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\" \"config/**/*.js\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zehedisode/PromtHubs.git"
  },
  "keywords": [
    "ai",
    "card-creator",
    "gemini",
    "imagen",
    "telegram-bot"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zehedisode/PromtHubs/issues"
  },
  "homepage": "https://github.com/zehedisode/PromtHubs#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.67.0",
    "sharp": "^0.34.5",
    "winston": "^3.19.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  }
}
